{
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#33ff33",
    "activityBar.background": "#33ff33",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#8080ff",
    "activityBarBadge.foreground": "#15202b",
    "commandCenter.border": "#15202b99",
    "editorGroup.border": "#33ff33",
    "panel.border": "#33ff33",
    "sash.hoverBorder": "#33ff33",
    "sideBar.border": "#33ff33",
    "statusBar.background": "#00ff00",
    "statusBar.border": "#00ff00",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#00cc00",
    "statusBarItem.remoteBackground": "#00ff00",
    "statusBarItem.remoteForeground": "#15202b",
    "tab.activeBorder": "#33ff33",
    "titleBar.activeBackground": "#00ff00",
    "titleBar.activeForeground": "#15202b",
    "titleBar.border": "#00ff00",
    "titleBar.inactiveBackground": "#00ff0099",
    "titleBar.inactiveForeground": "#15202b99"
  },
  "peacock.color": "lime",
  "files.exclude": {
    "**/.angular": true,
    "**/.storybook": true,
    "**/node_modules": true,
    "**/storybook-docgen": true,
    "**/.editorconfig": true,
    "**/.gitignore": true,
    "**/.vscode/extensions.json": true,
    "**/.vscode/launch.json": true,
    "**/.vscode/tasks.json": true,
    "angular.json": true,
    "package*": true,
    "**/*tsconfig.*": true,
    "**/*.md": true,
    "src/index.ts": true,
    "src/app/assets": true
  },





  // explorer.fileNesting
  //    Cannot settle on workspace or user settings for file nesting... yet (so why not both?)
  //    For now, duplicating the settings in both...
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.expand": true,
  "explorer.fileNesting.patterns": {
    "*.component.ts": "${basename}.ts, ${basename}.scss, ${basename}.spec.ts, ${basename}.html, ${basename}.stories.ts, ${basename}.mdx",
    "*.module.ts": "${capture}.module.ts, ${capture}-routing.module.ts",
    "*.service.ts": "${basename}.ts, ${basename}.spec.ts",
    "readme.md": "*.${extname}",
    "index.ts": "${basename}.ts, ${basename}.html",
    "*.scss": "_*.${extname}"
  }
}
